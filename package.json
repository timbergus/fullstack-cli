{
  "name": "fullstack-cli",
  "version": "0.0.0-development",
  "description": "Fullstack CLI to create and manage new full stack projects.",
  "main": "src/index.js",
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "hapi",
    "graphql",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timbergus/fullstack-cli.git"
  },
  "files": [
    "src",
    "package.json"
  ],
  "bin": {
    "fullstack-cli": "./src/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "panic": "rimraf node_modules && rimraf package-lock.json && npm install",
    "test": "npm run eslint && npm run sasslint && npm run jest",
    "codecov": "codecov",
    "test:watch": "jest --watch __tests__",
    "eslint": "eslint src",
    "sasslint": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "jest": "jest",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "eslint": "^5.8.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "jest": "^23.6.0",
    "sass-lint": "^1.12.1",
    "semantic-release": "^15.10.6",
    "travis-deploy-once": "^5.0.9"
  }
}
