{
  "name": "fullstack-cli",
  "version": "0.0.0-development",
  "description": "Fullstack CLI to create and manage new full stack projects.",
  "main": "src/index.js",
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "hapi",
    "graphql",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timbergus/fullstack-cli.git"
  },
  "files": [
    "src",
    "package.json"
  ],
  "bin": {
    "fullstack-cli": "./src/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "npm run eslint && npm run sass-lint && npm run jest",
    "codecov": "codecov",
    "test:watch": "jest --watch __tests__",
    "eslint": "eslint src",
    "sass-lint": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "jest": "jest --detectOpenHandles",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "panic": "rimraf package-lock.json && rimraf node_modules && npm i && npm test"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "mkdirp": "^1.0.4",
    "mustache": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.5",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "flow-bin": "^0.168.0",
    "jest": "^27.4.5",
    "sass-lint": "^1.13.1",
    "semantic-release": "^18.0.1",
    "travis-deploy-once": "^5.0.11"
  }
}
