{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "dist/index.js",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "library"
  ],
  "author": "{{{ author }}} <{{{ email }}}>",
  "license": "{{ license }}",
  "private": {{ private }},
  "config": {
    "build": "./scripts/build/build.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "styleguidist server",
    "test": "npm run eslint && npm run sass-lint && npm run jest",
    "test:watch": "jest --watch __tests__",
    "eslint": "eslint src",
    "sass-lint": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "jest": "jest --detectOpenHandles",
    "build": "npm run clean && npm test && cross-var node -r dotenv/config $npm_package_config_build",
    "build:styleguide": "styleguidist build",
    "prepublish": "npm run build",
    "panic": "rimraf package-lock.json && rimraf node_modules && npm i && npm test"
  },
  "dependencies": {
    "cpx": "^1.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "fs-extra": "^8.0.0",
    "glob": "^7.1.4",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "replace-in-file": "^4.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "cross-var": "^1.1.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.8.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "react-styleguidist": "^9.0.9",
    "rimraf": "^2.6.3",
    "sass": "^1.20.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0"
  }
}
